/*
class Node {
    int data;
    Node left, right;

    public Node(int data){
        this.data = data;
    }
}
*/
class Solution {
    public static int findMax(Node root) {
        if(root==null)
        {
            return -1;
        }
        int x=0;
        int left=findMax(root.left);
        int right=findMax(root.right);
        return Math.max(root.data,Math.max(left,right));
    }

    public static int main(Node root) {
        findMax(root);
    }
}
